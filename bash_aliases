# git stuff
alias m="mate"

alias gst="git status"
alias gst="git status"
alias gl="git pull"
alias gp="git push"
alias gd="git diff | mate"
alias gc="git commit -v"
alias gca="git commit -v -a"
alias gb="git branch"
alias gba="git branch -a"

alias hl="hg pull"
alias hst="hg status"
alias hp="hg push"
alias hc="hg commit"

alias ll="ls -alc"
